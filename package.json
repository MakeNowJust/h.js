{
  "name": "h.js",
  "description": "Only 2KB JavaScript Syntax Highlighter",
  "version": "5.1.0",
  "author": "TSUYUSATO Kitsune <make.just.on@gmail.com>",
  "bugs": {
    "url": "https://github.com/makenowjust/h.js/issues"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-plugin-eslint": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "ava": "3.3.0",
    "babel-eslint": "^10.0.3",
    "bulma": "0.8.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.1",
    "esm": "3.2.25",
    "highlight.js": "9.18.1",
    "prettier": "1.19.1",
    "raw-loader": "^4.0.0",
    "rollup": "1.31.1",
    "typeface-nothing-you-could-do": "^0.0.72",
    "typeface-share-tech-mono": "^0.0.72",
    "vue": "^2.6.11",
    "vue-template-compiler": "^2.6.11"
  },
  "files": [
    "h.js",
    "h.mjs"
  ],
  "homepage": "https://h.js.org/",
  "keywords": [
    "highlight",
    "syntax"
  ],
  "license": "MIT",
  "main": "h.js",
  "module": "h.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makenowjust/h.js.git"
  },
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix && prettier --list-different '**/*.{json,md}'",
    "serve": "vue-cli-service serve",
    "test": "ava"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "prettier",
      "prettier/vue"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "prettier/prettier": "error"
    }
  }
}
