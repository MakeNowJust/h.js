{
  "name": "h.js",
  "description": "Only 2KB JavaScript Syntax Highlighter",
  "version": "5.1.0",
  "author": "TSUYUSATO Kitsune <make.just.on@gmail.com>",
  "bugs": {
    "url": "https://github.com/makenowjust/h.js/issues"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.6",
    "@vue/cli-plugin-eslint": "4.4.6",
    "@vue/cli-service": "4.4.6",
    "ava": "3.10.1",
    "babel-eslint": "10.1.0",
    "eslint": "7.4.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-vue": "6.2.2",
    "esm": "3.2.25",
    "prettier": "2.0.5",
    "raw-loader": "4.0.1",
    "rollup": "2.20.0",
    "typeface-nothing-you-could-do": "0.0.72",
    "typeface-share-tech-mono": "0.0.72",
    "vue": "2.6.11",
    "vue-template-compiler": "2.6.11"
  },
  "files": [
    "h.js",
    "h.mjs"
  ],
  "homepage": "https://h.js.org/",
  "keywords": [
    "highlight",
    "syntax"
  ],
  "license": "MIT",
  "main": "h.js",
  "module": "h.mjs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/makenowjust/h.js.git"
  },
  "scripts": {
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix && prettier --list-different '**/*.{json,md}'",
    "serve": "vue-cli-service serve",
    "test": "ava"
  },
  "ava": {
    "require": [
      "esm"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "plugins": [
      "prettier"
    ],
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "prettier",
      "prettier/vue"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "prettier/prettier": "error"
    }
  }
}
